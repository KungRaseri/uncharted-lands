{
  "name": "uncharted-lands",
  "description": "Uncharted Lands is a game where players must build and manage settlements in a procedurally generated world. They must overcome various challenges such as extreme weather conditions, scarce resources, and hostile creatures, all while expanding their settlements and improving their technology.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "prisma generate && vite build",
    "migrate": "prisma migrate deploy",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "tsx prisma/seed"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.4.1",
    "@floating-ui/dom": "^1.5.4",
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@playwright/test": "^1.41.0",
    "@skeletonlabs/skeleton": "^2.7.1",
    "@skeletonlabs/tw-plugin": "^0.1.0",
    "@sveltejs/adapter-node": "^2.1.1",
    "@sveltejs/adapter-vercel": "^4.0.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/svelte": "^4.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^18.19.8",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vincjo/datatables": "^1.14.2",
    "@vitest/coverage-c8": "^0.28.5",
    "alea": "^1.0.1",
    "autoprefixer": "^10.4.17",
    "bcryptjs": "^2.4.3",
    "bson": "^4.7.2",
    "cuid": "^3.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.35.1",
    "fractal-noise": "^2.1.0",
    "jest-dom": "^4.0.0",
    "jsdom": "^21.1.2",
    "mdsvex": "^0.11.0",
    "node-cron": "^3.0.3",
    "open-simplex-noise": "^2.5.0",
    "postcss": "^8.4.33",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prisma": "^5.8.1",
    "redis": "^4.6.12",
    "simplex-noise": "^4.0.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "sveld": "^0.18.1",
    "svelte-canvas": "^1.0.2",
    "svelte-check": "^3.6.3",
    "svelte-material-icons": "^3.0.5",
